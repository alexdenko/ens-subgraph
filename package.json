{
  "name": "ens-subgraph",
  "version": "1.0.2",
  "repository": "https://github.com/ensdomains/ens-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "create-local": "graph create graphprotocol/ensGoerli --node http://127.0.0.1:8020",
    "remove-local": "graph remove graphprotocol/ensGoerli --node http://127.0.0.1:8020",
    "test": "graph test",
    "build": "graph build",
    "deploy-local": "graph deploy graphprotocol/ensGoerli --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020/",
    "deploy": "graph deploy ensdomains/ensGoerli --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/ensGoerli --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001",
    "setup": "yarn codegen && yarn create-local && yarn deploy-local",
    "docker:setup": "yarn codegen && yarn docker:create-local && yarn docker:deploy-local",
    "docker:create-local": "graph create graphprotocol/ensGoerli --node http://ens-app_graph-node_1:8020",
    "docker:deploy-local": "graph deploy graphprotocol/ensGoerli --debug --ipfs http://ens-app_ipfs_1:5001 --node http://ens-app_graph-node_1:8020/",
    "docker:remove-local": "graph remove graphprotocol/ensGoerli --node http://ens-app_graph-node_1:8020"
  },
  "devDependencies": {
    "@ensdomains/content-hash": "^2.5.3",
    "@graphprotocol/graph-cli": "^0.44.0",
    "@graphprotocol/graph-ts": "^0.29.1",
    "assemblyscript": "^0.26.3",
    "matchstick-as": "^0.5.0",
    "typescript": "^4.9.4"
  }
}
